{
  "name": "@aspirejo/cli",
  "version": "0.0.1",
  "description": "CLI for creating AspireJo standard projects",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/index.js",
    "dev": "npm run build && npm link"
  },
  "preferGlobal": true,
  "bin": {
    "aspire": "./dist/index.js",
    "asp": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AspireJo/cli.git"
  },
  "keywords": [
    "aspirejo",
    "cli",
    "node",
    "express",
    "swagger"
  ],
  "author": "AspireJo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AspireJo/cli/issues"
  },
  "homepage": "https://github.com/AspireJo/cli#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "cli": "^1.0.1",
    "commander": "^2.15.1",
    "inquirer": "^5.2.0",
    "lodash": "^4.17.10"
  }
}
